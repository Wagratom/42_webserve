<?php
// Obtém o valor do argumento passado
$listFiles = $argv[1];

// Divide a string em uma matriz de arquivos
$files = explode("\n", $listFiles);

// Remove o último elemento vazio da matriz
array_pop($files);

// Imprime a lista de arquivos como uma lista não ordenada em HTML
echo "<ul>";
foreach ($files as $file) {
    echo "<li>$file</li>";
}
echo "</ul>";
?>
